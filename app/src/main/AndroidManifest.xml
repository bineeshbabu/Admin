<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phacsin.admin">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".scan.ScanPayment"
            android:screenOrientation="portrait"/>
        <activity android:name=".scan.ScanParticipation"
            android:screenOrientation="portrait"/>
        <activity android:name=".scan.ScanPaymentView"
            android:screenOrientation="portrait"/>
        <activity android:name=".scan.ScanRegisterEvent"
            android:screenOrientation="portrait"/>
        <activity android:name=".main.SpinnerPage"/>
        <activity android:name=".main.MainActivity"/>

    </application>
</manifest>